# Table: `wp_mlw_results`
# Approximate rows expected in table: 0

# Delete any existing table `wp_mlw_results`

DROP TABLE IF EXISTS `wp_mlw_results`;

# Table structure of table `wp_mlw_results`

CREATE TABLE `wp_mlw_results` (
  `result_id` mediumint NOT NULL AUTO_INCREMENT,
  `quiz_id` int NOT NULL,
  `quiz_name` text NOT NULL,
  `quiz_system` int NOT NULL,
  `point_score` float NOT NULL,
  `correct_score` int NOT NULL,
  `correct` int NOT NULL,
  `total` int NOT NULL,
  `name` text NOT NULL,
  `business` text NOT NULL,
  `email` text NOT NULL,
  `phone` text NOT NULL,
  `user` int NOT NULL,
  `user_ip` text NOT NULL,
  `time_taken` text NOT NULL,
  `time_taken_real` datetime NOT NULL,
  `quiz_results` mediumtext NOT NULL,
  `deleted` int NOT NULL,
  `unique_id` varchar(255) NOT NULL,
  `form_type` int NOT NULL,
  PRIMARY KEY (`result_id`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8 ;

