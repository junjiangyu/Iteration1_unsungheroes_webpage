# Table: `wp_snowball_articles`
# Approximate rows expected in table: 0

# Delete any existing table `wp_snowball_articles`

DROP TABLE IF EXISTS `wp_snowball_articles`;

# Table structure of table `wp_snowball_articles`

CREATE TABLE `wp_snowball_articles` (
  `ID` mediumint NOT NULL AUTO_INCREMENT,
  `time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  `post_id` bigint NOT NULL,
  `article_html` longtext CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_520_ci,
  `preview_html` longtext CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_520_ci,
  `head_html` longtext CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_520_ci,
  `theme_option` tinyint(1) NOT NULL,
  PRIMARY KEY (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_520_ci ;

